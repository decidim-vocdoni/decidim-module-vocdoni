{
  "extends": "@decidim",
  "globals": {
    "process": true,
    "module": true
  },
  "rules": {
    "no-process-env": "off",
    "valid-jsdoc": "off"
  },
  "settings": {
    "react": {
      "version": "17.0.2"
    }
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "env": {
    "es2020": true
  }
}
